[General]
FullMachineName={{ansible_fqdn}}
SuiteSpotUserID=dirsrv
SuiteSpotGroup=dirsrv
AdminDomain=citc.acrc.bristol.ac.uk
ConfigDirectoryAdminID=admin
ConfigDirectoryAdminPwd={{ ldap_dm_password }}
ConfigDirectoryLdapURL=ldap://{{ansible_fqdn}}:389/o=NetscapeRoot

[slapd]
SlapdConfigForMC=Yes
UseExistingMC=0
ServerPort=389
ServerIdentifier=mgmt
Suffix=dc=citc,dc=acrc,dc=bristol,dc=ac,dc=uk
RootDN=cn=Directory Manager
RootDNPwd={{ ldap_dm_password }}
ds_bename=userRoot
AddSampleEntries=No

[admin]
Port=9830
#ServerIpAddress=
ServerAdminID=admin
ServerAdminPwd={{ ldap_dm_password }}
